package org.java;

public class SwapTwoNumbers {
public static void main(String[] args) {
	int a=10, b=20;
System.out.println("Before swapping.."+a+" "+b );	
          a=a*b;
          b=a/b;
          a=a/b;
System.out.println("After swapping..."+a+" "+b);

}
}


OUTPUT:
    Before swapping..10 20
    After swapping...20 10
